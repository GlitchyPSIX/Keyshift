<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>9</LangVersion>
    <Title>Keyshift</Title>
    <Copyright>Â© GLITCHYPSI 2022</Copyright>
    <PackageProjectUrl>https://github.com/GlitchyPSIX/Keyshift</PackageProjectUrl>
    <RepositoryUrl>https://github.com/GlitchyPSIX/Keyshift</RepositoryUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageTags>keyframe;dope sheet;sequencer;c#;library</PackageTags>
    <AssemblyVersion></AssemblyVersion>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <FileVersion></FileVersion>
    <Version>0.1.3</Version>
    <PackageReleaseNotes>Improved some issues with formatting, undo and performance.</PackageReleaseNotes>
    <PackageIcon>icon.png</PackageIcon>
    <Description>(WIP) C# Keyframe Timeline/Sequencer/Dope Sheet library.</Description>
    <Company>GLITCHYPSI</Company>
    <Authors>GlitchyPSI</Authors>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\.github\icon.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.2" />
    <PackageReference Include="System.Numerics.Vectors" Version="4.5.0" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="FormatHelpers\" />
  </ItemGroup>

</Project>
